{
  "name": "[[PROJECT_NAME]]",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "cd android && ./gradlew clean && cd .. && watchman watch-del-all && rm -rf node_modules && npm cache clean --force",
    "clean:install": "npm run clean && npm install",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace MyApp.xcworkspace -scheme MyApp -configuration Release",
    "pod:install": "cd ios && pod install",
    "postinstall": "pod-install --ios"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.0",
    "@react-native-async-storage/async-storage": "^1.19.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/native-stack": "^6.9.0",
    "react-native-safe-area-context": "^4.6.0",
    "react-native-screens": "^3.22.0",
    "react-native-vector-icons": "^10.0.0",
    "axios": "^1.5.0",
    "redux": "^4.2.1",
    "react-redux": "^8.1.0",
    "@reduxjs/toolkit": "^1.9.0",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-reanimated": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.72.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.0.0",
    "prettier": "^2.8.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-vector-icons)/)"
    ]
  }
}